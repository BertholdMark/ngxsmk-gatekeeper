{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": "c38be8d7-2a31-4100-b7a1-c994725ef804"
  },
  "newProjectRoot": "projects",
  "projects": {
    "ngxsmk-gatekeeper": {
      "projectType": "library",
      "root": "projects/ngxsmk-gatekeeper",
      "sourceRoot": "projects/ngxsmk-gatekeeper/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ngxsmk-gatekeeper/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngxsmk-gatekeeper/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ngxsmk-gatekeeper/tsconfig.spec.json"
          }
        }
      }
    }
  }
}

